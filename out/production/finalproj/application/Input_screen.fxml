<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="457.0" prefWidth="423.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.number_of_nodes">
   <children>
      <Pane prefHeight="472.0" prefWidth="544.0">
         <children>
            <TextField fx:id="nodes_textField" layoutX="30.0" layoutY="52.0" onKeyPressed="#getNumOfNodesKey" prefHeight="31.0" prefWidth="106.0" promptText="0">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Label layoutX="30.0" layoutY="14.0" prefHeight="37.0" prefWidth="450.0" text="Enter the number of nodes and press the button">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="nodes_button" layoutX="168.0" layoutY="52.0" mnemonicParsing="false" onAction="#getNumOfNodes" text="enter">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
